{
  "build": {
    "dockerfile": "Dockerfile"
  },
  "run": {
    "image": "docker.io/ubuntu:latest"
  },
  "forwardPorts": [3000, 4000, 5000, 8000, 8080],
  "name": "Dart SDK",
  "customizations": {
    "vscode": {
      "extensions": ["Dart-Code.dart-code"]
    },
    "settings": {
      "terminal.integrated.shell.linux": "/bin/bash"
    },
    "remoteUser": "root",
    "runArgs": ["--privileged"]
  }
}
